host:
netserver -D -4 -L 0.0.0.0 -p 12865

client:
netperf -H 10.2.1.7 -j -c -l -1000 -t TCP_RR -- -D -O THROUGHPUT_UNITS,THROUGHPUT,MEAN_LATENCY,MIN_LATENCY,MAX_LATENCY,P50_LATENCY,P90_LATENCY,P99_LATENCY,STDDEV_LATENCY,LOCAL_CPU_UTIL

